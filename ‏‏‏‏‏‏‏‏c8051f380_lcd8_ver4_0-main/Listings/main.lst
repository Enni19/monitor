 409          float goertzel_mag(int numSamples,int TARGET_FREQUENCY,int SAMPLING_RATE)
 410          {
 411   1      
 412   1        int     k,i;
 413   1        float analog;
 414   1        float   floatnumSamples;
 415   1        float   omega,sine,cosine,coeff,q0,q1,q2,magnitude,real,imag;
 416   1        float   scalingFactor = numSamples / 2.0;
 417   1      
 418   1        floatnumSamples = (float) numSamples;
 419   1        k = (int) (0.5 + ((floatnumSamples * TARGET_FREQUENCY) / SAMPLING_RATE));
 420   1        omega = (2.0 * 3.14 * k) / floatnumSamples;
 421   1        sine = sin(omega);
C51 COMPILER V9.59.0.0   MAIN                                                              04/24/2023 10:47:53 PAGE 8   

 422   1        cosine = cos(omega);
 423   1        coeff = 2.0 * cosine;
 424   1        q0=0;
 425   1        q1=0;
 426   1        q2=0;
 427   1      
 428   1        for(i=0; i<numSamples; i++)
 429   1        {
 430   2          analog=data_in[i];
 431   2          q0 = coeff * q1 - q2 + analog;
 432   2          q2 = q1;
 433   2          q1 = q0;
 434   2        }
 435   1      
 436   1        // calculate the real and imaginary results
 437   1        // scaling appropriately
 438   1        real = (q1 - q2 * cosine) / scalingFactor;
 439   1        imag = (q2 * sine) / scalingFactor;
 440   1      
 441   1        magnitude = sqrt(real*real + imag*imag);
 442   1      
 443   1        return magnitude;
 444   1      }
